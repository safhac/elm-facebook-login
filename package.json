{
  "name": "elm-facebook-api",
  "version": "0.0.3",
  "description": "Elm app based on webpack starter kit to authenticate users with Facebook and save login status in localstorage",
  "main": "src/main.js",
  "source-directories": "./src",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-live": "elm-live src/Main.elm --output=src/main.js --port=8000 --open",
    "start": "webpack-dev-server --hot --inline --content-base src/",
    "build": "rimraf dist && webpack",
    "reinstall": "rimraf node_modules && npm uninstall -g elm && npm i -g elm && npm i && elm package install"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "bootstrap-sass": "^3.3.6",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.25.0",
    "elm": "^0.18.0",
    "elm-hot-loader": "^0.5.2",
    "elm-webpack-loader": "^4.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.17.0",
    "jquery": "^3.1.0",
    "node-sass": "^3.7.0",
    "postcss-loader": "^1.1.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.16.0"
  },
  "keywords": [
    "Elm",
    "Facebook",
    "Webpack"
  ],
  "author": "Asaf Cohen <safhac@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:safhac/elm-facebook-login"
  },
  "dependencies": {
    "live-server-https": "^0.0.2",
    "node-gyp": "^3.8.0"
  }
}
